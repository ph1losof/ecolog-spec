# Multiple equals signs (first = is separator)
CONNECTION_STRING=Server=localhost
Port=5432
Database=mydb
EQUATION=x=y+z
DOUBLE_EQUALS==value

# Whitespace in various positions
LEADING_SPACE= value_with_leading_spaces
TRAILING_TRIMMED=value
PRESERVED_IN_QUOTES="  spaces preserved  "

# Quotes in unquoted values (literal)
UNQUOTED_QUOTES=foo"bar"baz
MIXED=foo'bar'baz

# Nested quotes
OUTER_DOUBLE="outer 'inner' end"
OUTER_SINGLE='outer "inner" end'

# Consecutive backslashes in continuation
ESCAPED_BACKSLASH=value\\
continued

# Empty continuation lines
CONTINUATION=start
middle
end

# Duplicate keys (last wins)
OVERRIDE=first
OVERRIDE=second
OVERRIDE=final_value

# Case sensitivity
api_key=lowercase
API_KEY=uppercase
Api_Key=mixedcase

# Special characters in unquoted values
UNDERSCORE=value_with_underscores
DASH_URL=http://example.com/path-with-dashes
DOT_HOST=api.example.com
AT_EMAIL=user@example.com
